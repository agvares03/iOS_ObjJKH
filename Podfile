target 'ObjJKH' do
    
    use_frameworks!

  # Pods for Мобильное ЖКХ
  pod 'Firebase/Core'
  pod 'Firebase/Messaging'
  pod 'Alamofire', :git => 'https://github.com/Alamofire/Alamofire.git'
  pod 'SwiftyXMLParser', :git => 'https://github.com/yahoojapan/SwiftyXMLParser.git'
  pod 'Gloss'
  pod 'Dropper'

end

